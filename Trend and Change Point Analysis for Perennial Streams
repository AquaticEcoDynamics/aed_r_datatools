annualbaseflowtrend4<- annualbaseflowtrend4+geom_text(mapping = aes(x = Mid_date,
                                                                    y = max(sum_baseflow),
                                                                    fill=`Sens Slope`,
                                                                    label = Line.pvalue,
                                                                    hjust =1,
                                                                    vjust = 5.5),
                                                      size=2.5,
                                                      angle=90,
                                                      show.legend = FALSE,
                                                      data = total_baseflow_annuall)
