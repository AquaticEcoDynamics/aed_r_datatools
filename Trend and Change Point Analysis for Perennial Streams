s.res <- lanzante.test(Baseflow_wetseason$sum_baseflow)
n <- s.res$nobs
i <- s.res$estimate
s.1 <- mean( Baseflow_wetseason$sum_baseflow[1:i])
s.2 <- mean(Baseflow_wetseason$sum_baseflow[(i+1):n])
sw<-(c(rep(s.1,i), rep(s.2,(n-i))))
