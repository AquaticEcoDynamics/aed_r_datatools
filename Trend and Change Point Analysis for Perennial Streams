s.res <- lanzante.test(Flow_dryseason$sum_flow)
n <- s.res$nobs
i <- s.res$estimate
s.1 <- mean( Flow_dryseason$sum_flow[1:i])
s.2 <- mean(Flow_dryseason$sum_flow[(i+1):n])
s <-(c(rep(s.1,i), rep(s.2,(n-i))))
lines(Flow_dryseason$Year, s, col=2,lty=1)
print(s.res)
