  geom_point(aes(y=sum_flow, color=`Sens Slope`),size=1)+xlab("Date")+ylab("Dry Runoff (mm/yr)")+
  ggtitle("")+theme_bw()+theme(plot.title = element_text(hjust =0.5 ))

